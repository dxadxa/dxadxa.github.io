    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/30617874-8844-43ec-a6d6-9b61670dd76c"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/39538f6d-a8b9-4f7a-90e7-23c5faf53227"/>
