    <dcterms:title xml:lang="en">Quality checked atmospheric carbon dioxide concentration measured at IPR-1</dcterms:title>
    <dcat:accessURL rdf:resource="https://cidportal.jrc.ec.europa.eu/ftp/jrc-opendata/ABCIS/GreenHouseGases/FinalData_IPR-1/CO2/IPR_1_1_CO2_1_20071012_20180223.txt"/>
