    <dcterms:title xml:lang="en">Process steam from heavy fuel oil; heat plant; consumption mix, at plant; MJ (Location: DE)</dcterms:title>
    <dcatapop:ckanName>jrc-eplca-3175e00f-a6d5-46a1-a939-c46ba99343bc</dcatapop:ckanName>
