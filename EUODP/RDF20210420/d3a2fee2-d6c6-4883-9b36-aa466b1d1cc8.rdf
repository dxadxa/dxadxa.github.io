    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <dcterms:title xml:lang="en">Dissemination and implementation of the new food safety tool for exposure assessment - Flavourings, Additives and food Contact materials Exposure Tool (FACET)</dcterms:title>
    <schema:url xml:lang="en">https://ec.europa.eu/jrc/en/publication/eur-scientific-and-technical-research-reports/dissemination-and-implementation-new-food-safety-tool-exposure-assessment-flavourings</schema:url>
