  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/EPOd7hutpXPNf0HIDvxOFQ">
    <dcterms:title xml:lang="de">Körperliche und sexuelle Gewalt gegen Frauen, nach Altersgruppe (Daten von 2012) (Quelle: FRA)</dcterms:title>
    <dcterms:title xml:lang="fr">Violences physiques et sexuelles à l'égard des femmes, par classe d'âge (données de 2012) (source: FRA)</dcterms:title>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
    <dcatapop:ckanName>EPOd7hutpXPNf0HIDvxOFQ</dcatapop:ckanName>
