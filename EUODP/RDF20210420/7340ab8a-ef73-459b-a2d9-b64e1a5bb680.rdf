  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/7340ab8a-ef73-459b-a2d9-b64e1a5bb680">
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/bcf45f43-6edc-4a19-a247-a5c05810e8b6"/>
