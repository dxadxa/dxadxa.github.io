  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/fPJrLmzWS3W4erpOXS4OQQ">
    <dcterms:title xml:lang="fr">Transactions internationales de transport: exports, imports et balance</dcterms:title>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/a11d4737-52f4-4262-b942-2b5f3e5bac18"/>
    <dcat:keyword xml:lang="en">international trade</dcat:keyword>
