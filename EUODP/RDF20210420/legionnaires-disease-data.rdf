  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/d52ee232-f9a4-4e93-9aae-469dc7b0a3da">
    <dcat:downloadURL rdf:resource="http://ecdc.europa.eu/sites/portal/files/documents/AER_for_2015-legionnaires-disease_0.pdf"/>
