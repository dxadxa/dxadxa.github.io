    <dcterms:title xml:lang="fr">Personnes en risque de pauvreté ou d'exclusion sociale par degré d'urbanisation</dcterms:title>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/4050"/>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/2c76b8e7-0af8-42d0-90ac-2ca2e4139f8e"/>
