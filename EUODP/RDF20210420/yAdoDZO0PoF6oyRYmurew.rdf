    <dcterms:title xml:lang="de">Leistungsbilanz, Hauptkomponenten, Saldo - vierteljährliche Daten, Millionen Landeswährung</dcterms:title>
    <dcatapop:ckanName>yAdoDZO0PoF6oyRYmurew</dcatapop:ckanName>
