    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/d6f2a852-4437-4d5d-a273-7f2744bee74d"/>
    <dcterms:title xml:lang="en">Gross value added - NACE Rev. 2: B-E - current prices</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/c9491d52-e229-495e-97df-cdc6b2c8499f"/>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/56"/>
