  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/S2260_485_ENG">
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/0ba7d05a-6cb4-4517-81bf-f1855e351b72"/>
