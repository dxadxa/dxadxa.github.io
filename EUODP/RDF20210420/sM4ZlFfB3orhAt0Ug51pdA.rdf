  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/sM4ZlFfB3orhAt0Ug51pdA">
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/84a70351-6f96-437e-ace1-7241e024c5ba"/>
