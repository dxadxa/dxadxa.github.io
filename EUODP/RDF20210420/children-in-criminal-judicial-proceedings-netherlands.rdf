  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/children-in-criminal-judicial-proceedings-netherlands">
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/dataset-type/STATISTICAL"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/12757cb6-b975-4a65-ae1b-800160c7b5e9"/>
