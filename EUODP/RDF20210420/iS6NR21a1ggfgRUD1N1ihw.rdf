    <dcterms:title xml:lang="de">Arbeitsproduktivität je Beschäftigteund und geleistete Arbeitsstunde (EU27_2020=100)</dcterms:title>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/29719ad2-a8b3-48cc-a507-fe52f307ed40"/>
