  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/LZFNc9iK5CqZ7GnPPWbw">
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/dataset-type/STATISTICAL"/>
    <dcterms:title xml:lang="de">Erwerbstätigenquote älterer Erwerbstätiger</dcterms:title>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/04c79f8b-a1bc-41d4-95dd-551ece4ba009"/>
