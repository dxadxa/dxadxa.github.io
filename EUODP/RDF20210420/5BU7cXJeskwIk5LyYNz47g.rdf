    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/3eb26e12-bcb9-4714-8f75-0b8e9379d7db"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/7c816f7a-b366-4432-b4a7-8839c1e58086"/>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
