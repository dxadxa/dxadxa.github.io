  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/ad8d6f0c-35ca-453d-8070-6eca058f9d44">
    <dcat:accessURL rdf:resource="https://www.eurofound.europa.eu/publications/report/2009/quality-of-life-social-policies/second-european-quality-of-life-survey-quality-of-life-in-europe-2003-2007"/>
