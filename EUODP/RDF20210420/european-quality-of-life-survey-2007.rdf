  <rdf:Description rdf:about="http://data.europa.eu/88u/document/64ee37a8-e355-4c14-bb45-09b5d9a02372">
    <foaf:topic rdf:resource="http://data.europa.eu/88u/dataset/european-quality-of-life-survey-2007"/>
