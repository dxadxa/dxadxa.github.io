  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/fea7d639-8cf5-4523-9040-55585733eea8">
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-02-12 17:01:38.330662</dcterms:issued>
