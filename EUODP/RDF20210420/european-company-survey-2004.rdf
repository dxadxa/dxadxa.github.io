  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/3d98a63e-6f34-4fb7-b778-c211d59e27c4">
    <adms:status rdf:resource="http://publications.europa.eu/resource/authority/dataset-status/COMPLETED"/>
