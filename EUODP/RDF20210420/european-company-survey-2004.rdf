    <dcat:accessURL rdf:resource="https://www.eurofound.europa.eu/publications/report/2007/working-conditions/extended-and-unusual-working-hours-in-european-companies"/>
    <dcterms:license rdf:resource="http://publications.europa.eu/resource/authority/licence/COM_REUSE"/>
  </rdf:Description>
