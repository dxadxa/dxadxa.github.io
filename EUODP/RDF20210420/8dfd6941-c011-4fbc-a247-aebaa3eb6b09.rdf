  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/3dcf1dde-4023-492d-95b3-3131d0862980">
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/XLS"/>
