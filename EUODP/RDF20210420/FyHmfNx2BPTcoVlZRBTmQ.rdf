  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/FyHmfNx2BPTcoVlZRBTmQ">
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/c9257c34-21c6-41ee-8051-0356f924539f"/>
