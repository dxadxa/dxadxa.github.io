  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/at2AAzgg68oaGy5DBl8Sg">
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/1280"/>
    <dcterms:title xml:lang="en">Standardised death rate due to homicide by sex</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/349ad7bc-ae09-4629-9922-5865ef31c2b5"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
