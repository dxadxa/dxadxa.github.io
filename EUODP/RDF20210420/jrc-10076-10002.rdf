  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/00a2f8af-c053-48de-8b65-591370bfe2aa">
    <dcterms:description xml:lang="en">The data are provided in the form of raster layers (geotiff) representing the theoretical and collectible potentials of livestock resources (manure and methane) in Europe at 1km of spatial resolution.</dcterms:description>
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/distribution-type/DOWNLOADABLE_FILE"/>
    <dcterms:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC_BY_4_0"/>
    <dcat:accessURL rdf:resource="https://cidportal.jrc.ec.europa.eu/ftp/jrc-opendata/GIS-RE/10002/LATEST/geotiff/"/>
    <dcterms:title xml:lang="en">Spatial distribution of livestock resources in Europe</dcterms:title>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/OP_DATPRO"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/jrc-10076-10002">
    <dcterms:spatial rdf:resource="http://publications.europa.eu/resource/authority/country/LUX"/>
    <dcterms:spatial rdf:resource="http://publications.europa.eu/resource/authority/country/BGR"/>
    <dcat:keyword xml:lang="en">Science and technology</dcat:keyword>
    <dcterms:description xml:lang="en">This study performed a spatially explicit assessment of biogas potential of farm manure in Europe at 1 km spatial resolution. The Gridded Livestock of the World (GLW) database from FAO was used as spatial proxy for the spatial distribution of the livestock. The results include the estimation of the theoretical and realistic potentials for manure (k tonnes) and methane (TJ).</dcterms:description>
