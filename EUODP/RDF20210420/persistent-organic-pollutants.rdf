  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/f211ae14-63ab-49fc-ba7f-bea0fc7db83a">
    <dcat:downloadURL rdf:resource="https://echa.europa.eu/list-of-substances-proposed-as-pops"/>
