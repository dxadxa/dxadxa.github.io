    <foaf:page rdf:resource="http://data.europa.eu/88u/document/986f29c3-f427-419f-a98e-60b2f5777c3b"/>
    <dcterms:title xml:lang="fr">Nuitées de résidents et non-résidents dans des établissements d'hébergement touristique - données mensuelles</dcterms:title>
    <dcatapop:ckanName>8q8khKJ7iehKkpG7Sn16Dw</dcatapop:ckanName>
