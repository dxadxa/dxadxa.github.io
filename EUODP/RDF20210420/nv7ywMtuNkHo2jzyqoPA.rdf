    <dcterms:title xml:lang="en">[DISCONTINUED] Individuals' level of internet skills</dcterms:title>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/HEAL"/>
    <dcatapop:isPartOfCatalog rdf:resource="http://data.europa.eu/88u/catalog/euodp"/>
