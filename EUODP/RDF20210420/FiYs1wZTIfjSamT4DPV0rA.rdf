    <dcterms:title xml:lang="de">Defizit/Überschuss, Schuldenstand des Staates</dcterms:title>
    <dcat:keyword xml:lang="en">Finance</dcat:keyword>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/67535b75-3416-4a4e-8231-17cb8668a4f6"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/540715a8-7e1c-4ef7-9476-91f88428695f"/>
    <dcat:keyword xml:lang="en">Public finance</dcat:keyword>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/REGI"/>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/e7ba1849-5dc0-4cf6-93cf-d016e7a55b9d"/>
