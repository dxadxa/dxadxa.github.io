  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/jrc-10112-10010">
    <dcterms:identifier>jrc-10112-10010</dcterms:identifier>
    <dcterms:title xml:lang="en">Global land surface phenology - Start of the season</dcterms:title>
