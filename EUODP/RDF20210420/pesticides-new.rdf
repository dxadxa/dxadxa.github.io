    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/3469"/>
    <dcat:keyword xml:lang="en">approximation of laws</dcat:keyword>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/dffc8683-f9e9-46cc-9e76-100341a3a51c"/>
