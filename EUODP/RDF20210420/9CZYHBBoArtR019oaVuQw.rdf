    <dcterms:title xml:lang="de">Schüler die Französisch lernen</dcterms:title>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/347d90a4-ef38-40c0-ae1a-8747a63a1f60"/>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/SOCI"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/9c48e67e-c9cc-4e82-a6d0-f7f90eaf77b5"/>
    <dcterms:identifier>tps00058</dcterms:identifier>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/02de7f96-9d92-4dce-9a05-67638f9eb532"/>
    <dcterms:title xml:lang="en">Pupils learning French</dcterms:title>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-10-24</dcterms:modified>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/d168143f-fce1-4024-b8e1-b34700862418"/>
    <dcterms:description xml:lang="fr">Cet indicateur présente le pourcentage de l'ensemble des élèves en deuxième cycle de l'enseignement secondaire (niveau 3 de la CITE) qui apprennent le français comme langue étrangère. Il concerne uniquement l'enseignement général, et non l'enseignement professionnel, dans des pays où le français est décrit comme une langue étrangère dans les programmes scolaires ou dans d'autres documents officiels relatifs à l'éducation dans le pays.</dcterms:description>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
    <dcat:keyword xml:lang="en">education</dcat:keyword>
    <dcat:keyword xml:lang="en">foreign language</dcat:keyword>
  </rdf:Description>
