  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/QU9vepMWApyNOY8lyhFPA">
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/5ae4bfb0-a09a-4930-b0b0-c0c810f94734"/>
