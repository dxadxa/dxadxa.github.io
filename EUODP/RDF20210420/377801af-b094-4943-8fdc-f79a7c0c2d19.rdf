    <dcterms:title xml:lang="en">EDGAR v5.0 Global Air Pollutant Emissions</dcterms:title>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/5878cb09-f67e-425b-84d5-8efc1adc8f0b"/>
