  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/VMKWnopxMItKrZHU1Ma2A">
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/f7739da2-5f00-47eb-960c-fb6f211eda52"/>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-02-08</dcterms:modified>
