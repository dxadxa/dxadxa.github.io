  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/S2035_406">
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/5a2ee0fb-a592-4048-91c9-662e8b2d6c30"/>
