  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/E3OPzCkdU9h0mSQw5Jg">
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/4c3684f7-1a6b-4520-b020-e6241a7e962c"/>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/ec9427c0-50ef-4248-967d-f64f5d23338e"/>
