    <dcterms:title xml:lang="fr">Main-d'oeuvre dans la construction - Index (2010 =100)</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/83a656c8-9bad-41f1-8b10-3914f7412dc8"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/42164532-f87c-4f1e-aa40-f5704aa45656"/>
