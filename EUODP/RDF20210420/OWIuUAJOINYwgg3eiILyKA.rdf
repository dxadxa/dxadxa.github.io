    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/9369b56d-2f06-4834-81dd-35f61d4c4b6f"/>
    <dcterms:title xml:lang="fr">Impôts courants sur le revenu et le patrimoine</dcterms:title>
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/1995b4b3-2219-4e37-b57d-2b3eb8000e16"/>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-02-15</dcterms:modified>
