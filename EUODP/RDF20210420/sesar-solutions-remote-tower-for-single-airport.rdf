    <dcat:accessURL rdf:resource="http://www.sesarju.eu/sites/default/files/solutions/4_Single_Remote_Tower_OSED.pdf?issuusl=ignore"/>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/PDF"/>
