  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/2a313369-6386-4a39-988b-521dcdd493e3">
    <dcat:downloadURL rdf:resource="http://www.sesarju.eu/sites/default/files/solutions/3a_Single_Remote_Tower_Human_Performance_Assessment.pdf?issuusl=ignore"/>
