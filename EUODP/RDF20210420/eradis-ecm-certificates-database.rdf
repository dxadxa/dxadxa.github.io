  <rdf:Description rdf:about="http://data.europa.eu/88u/document/ee99e92f-291b-4c15-b82c-f935f05fe35e">
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/documentation-type/DOCUMENTATION_MAIN"/>
