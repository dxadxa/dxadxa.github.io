    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/3140"/>
    <dcterms:title xml:lang="en">Normalized Water-Leaving Radiance determined from above-water radiometry - Gloria (44N,29E)</dcterms:title>
    <dcterms:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
