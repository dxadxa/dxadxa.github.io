    <dcterms:title xml:lang="en">Economic sentiment indicator</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/2de4069c-0e7d-4cfa-83ab-f5ff539d2bff"/>
    <dcterms:title xml:lang="fr">Indicateur de sentiment économique</dcterms:title>
    <dcat:keyword xml:lang="en">business surveys</dcat:keyword>
