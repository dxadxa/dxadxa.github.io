  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/d751da57-f8ac-4c0d-9a7d-e1a7ca4fb791">
    <dcterms:issued xml:lang="en">2014-12-18 08:15:26.062083</dcterms:issued>
