    <dcat:accessURL rdf:resource="https://www.usegalileo.eu/EN/inner.html#data=road"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/88u/document/b2d9680a-fae9-4a90-8700-ec2b4d793748">
