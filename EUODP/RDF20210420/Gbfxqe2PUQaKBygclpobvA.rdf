  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/Gbfxqe2PUQaKBygclpobvA">
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/34f5cc17-37fa-4c35-afd4-72fceb406e30"/>
