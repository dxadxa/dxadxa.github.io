    <dcterms:title xml:lang="fr">Ventes totales de pesticides</dcterms:title>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/4260"/>
    <dcterms:description xml:lang="de">Menge der in den Mitgliedstaaten verkauften Pestizide. Diese Datenerhebung umfasst Pestizid-Verkaufsdaten in EU27 (ab 2020) und EFTA (Schweiz und Norwegen) auf der Grundlage des Begriffs "Inverkehrbringen" im Sinne der Verordnung (EG) Nr. 1107/2009 des Europäischen Parlaments und des Rates vom 21. Oktober 2009 über das Inverkehrbringen von Pflanzenschutzmitteln.</dcterms:description>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/ed25c827-b6b5-4768-a6e3-38344da2ec05"/>
    <dcat:keyword xml:lang="en">environmental statistics</dcat:keyword>
    <dcterms:title xml:lang="en">Sales of pesticides by type of pesticide</dcterms:title>
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/dataset-type/STATISTICAL"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/60b0ccd2-a636-41d4-95b4-10692ff6f408"/>
    <dcterms:title xml:lang="de">Gesamtabsatz von Schädlingsbekämpfungsmitteln</dcterms:title>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/6331"/>
