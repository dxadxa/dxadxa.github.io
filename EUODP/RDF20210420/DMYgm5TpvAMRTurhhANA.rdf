    <dcterms:title xml:lang="de">Direktinvestitionen im Ausland in % des BIP</dcterms:title>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
    <dcterms:title xml:lang="fr">Stocks sortants des investissements directs étrangers (IDE) en % du PIB</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/711828fa-4ae6-481a-9dc8-577662f2be44"/>
