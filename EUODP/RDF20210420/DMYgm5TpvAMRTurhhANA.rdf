  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/DMYgm5TpvAMRTurhhANA">
    <dcterms:title xml:lang="en">Outward FDI stocks in % of GDP</dcterms:title>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <dcatapop:ckanName>DMYgm5TpvAMRTurhhANA</dcatapop:ckanName>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/827b1ecc-e112-4fa5-a105-b108fad86863"/>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/4671"/>
