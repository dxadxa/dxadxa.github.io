    <dcterms:title xml:lang="en">Analytical report: Life satisfaction, happiness and sense of belonging</dcterms:title>
    <dcat:downloadURL rdf:resource="https://www.eurofound.europa.eu/publications/report/2005/quality-of-life/first-european-quality-of-life-survey-life-satisfaction-happiness-and-sense-of-belonging"/>
    <adms:status rdf:resource="http://publications.europa.eu/resource/authority/dataset-status/COMPLETED"/>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/PDF"/>
    <dcat:accessURL rdf:resource="https://www.eurofound.europa.eu/publications/report/2005/quality-of-life/first-european-quality-of-life-survey-life-satisfaction-happiness-and-sense-of-belonging"/>
