    <dcterms:title xml:lang="de">Gesamtausgaben für Forschung und Entwicklung</dcterms:title>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/8776a5e8-0a0c-4638-9134-e5675165b250"/>
    <dcterms:modified>2018-08-31 13:06:41.046921</dcterms:modified>
