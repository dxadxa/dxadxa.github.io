  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/jrc-luisa-lf441-potential-accessibility-ref-2014">
    <dcterms:spatial rdf:resource="http://publications.europa.eu/resource/authority/country/ROU"/>
