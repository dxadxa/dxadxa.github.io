    <dcterms:title xml:lang="en">CSV conversion tool</dcterms:title>
    <dcat:accessURL rdf:resource="http://csvdownloader.iatistandard.org/csv/direct_from_registry/?xml=https%3A//webgate.ec.europa.eu/hac/IATI/ec-echo-2012.xml"/>
