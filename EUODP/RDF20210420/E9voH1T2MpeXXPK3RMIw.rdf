  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/E9voH1T2MpeXXPK3RMIw">
    <dcterms:title xml:lang="en">International investment position - quarterly data</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/12f458fb-31aa-40d9-b218-908a169a2717"/>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/4671"/>
