    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/6bc3b807-0649-46d4-86f4-d50bd46b7811"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/53662bfd-8693-4a0a-a06d-0c373f2f478d"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/29fed23e-bb8e-40b1-ae87-3bc71672dd7d"/>
