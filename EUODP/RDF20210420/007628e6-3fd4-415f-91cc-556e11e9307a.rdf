    <dcterms:title xml:lang="de">Umsatz - Einzelhandel ohne Nahrungs- und Genussmittel, Getränke und Tabakwaren</dcterms:title>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/bca276cd-5432-4009-a094-fea84795f0a4"/>
