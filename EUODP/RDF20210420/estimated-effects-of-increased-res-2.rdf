    <dcterms:title xml:lang="en">[DEPRECATED] Estimated effects of increased RES consumption since 2005 on fossil fuels and GHG emissions</dcterms:title>
    <adms:status rdf:resource="http://publications.europa.eu/resource/authority/dataset-status/DEPRECATED"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/a7734fde-590b-44bb-a8c6-a22faabe48cf"/>
