    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2017-01-23 14:36:37.507345</dcterms:issued>
    <dcat:accessURL rdf:resource="https://e-justice.europa.eu/content_crossborder_family_mediation-387-fr-en.do?member=1"/>
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/distribution-type/FEED_INFO"/>
