  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/SSMXN2Ek3mwBylrhI6WgLw">
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/513bda8a-5eb5-414c-a96e-dc0b9cca09d3"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/d8de7440-f803-475a-a2f8-86ad39cd10dc"/>
    <dcterms:title xml:lang="fr">Investissements pour la protection de l'environnement de l'ensemble de l'économie</dcterms:title>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/6402"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/b2c73ee1-e834-4d71-8b9f-5dd75ac34faf"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/e7b8f969-852d-4878-b412-4b897d8a4e13"/>
    <dcterms:description xml:lang="en">This table presents investments of total economy (general governments and corporations) to provide environmental protection services (e.g. waste and wastewater management, decontamination of soil). Investments undertaken by corporations to manage their own environmental pressures are included.</dcterms:description>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/ENVI"/>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
