    <dcterms:title xml:lang="de">Leistungsbilanz, Hauptkomponenten, Debet - jährliche Daten, % des BIP</dcterms:title>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/ae9b9bdf-9ff8-4e80-a20c-d78c6f9a2319"/>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/4671"/>
  </rdf:Description>
