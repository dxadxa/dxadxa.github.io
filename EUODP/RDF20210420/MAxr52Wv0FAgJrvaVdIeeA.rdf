    <dcterms:title xml:lang="en">Compensation of employees</dcterms:title>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/45091625-589c-47a3-96f1-c1a18bb2a105"/>
