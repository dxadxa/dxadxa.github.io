    <dcterms:title xml:lang="en">[DEPRECATED] Management related pressures on forest ecosystems</dcterms:title>
    <foaf:topic rdf:resource="http://data.europa.eu/88u/dataset/management-related-pressures-on-forest-ecosystems"/>
