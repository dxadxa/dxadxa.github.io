  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/two-years-until-the-2019-european-elections">
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/c73e7750-7d1f-4f22-82e1-75d5273a43dd"/>
