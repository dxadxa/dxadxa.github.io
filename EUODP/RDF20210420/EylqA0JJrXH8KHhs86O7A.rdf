    <dcterms:title xml:lang="fr">Dépenses de consommation finale des administrations publiques, volumes</dcterms:title>
    <dcterms:title xml:lang="en">Government final consumption expenditure, volumes</dcterms:title>
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/dataset-type/STATISTICAL"/>
