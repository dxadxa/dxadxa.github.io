    <dcterms:title xml:lang="fr">Parts de marché à l'exportation - variation en % sur 5 ans</dcterms:title>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/fda8c1f7-86b8-4cee-af36-827fc0da570b"/>
