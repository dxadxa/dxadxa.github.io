    <dcterms:title xml:lang="en">Normalized Water-Leaving Radiance determined from above-water radiometry - Helsinki_Lighthouse (59N,24E)</dcterms:title>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/3140"/>
