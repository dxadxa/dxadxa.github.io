  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/dic3BfR6gitITQ5gnxEnsQ">
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/f9d4c4a7-7402-44c2-8d9e-b74628435aca"/>
