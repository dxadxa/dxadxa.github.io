    <dcterms:issued xml:lang="en">2019-01-07 16:06:48.965612</dcterms:issued>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/PDF"/>
    <dcterms:license rdf:resource="http://publications.europa.eu/resource/authority/licence/COM_REUSE"/>
    <dcat:accessURL rdf:resource="http://ecdc.europa.eu/sites/portal/files/documents/seasonal-influenza-annual-epidemiological-report-2017.pdf"/>
    <adms:status rdf:resource="http://publications.europa.eu/resource/authority/dataset-status/COMPLETED"/>
    <dcterms:description xml:lang="en">During the 2017–2018 season, 41 315 specimens from sentinel primary care providers were tested for influenza, 22% more than in the previous season; 49% of the specimens were positive for influenza virus (previous season: 40%).</dcterms:description>
