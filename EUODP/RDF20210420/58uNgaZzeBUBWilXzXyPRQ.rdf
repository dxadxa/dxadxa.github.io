  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/58uNgaZzeBUBWilXzXyPRQ">
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/b85f89ba-242a-46f0-b52f-7fed43ebc5a0"/>
