    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/d92cd4b0-f891-40d6-91f6-e20ca54600f5"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
