  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/jrc-esdac-1">
    <dcterms:title xml:lang="en">European Soil Database v2.0 (vector and attribute data)</dcterms:title>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2001-01-01</dcterms:issued>
    <dcat:keyword xml:lang="en">European Soil Database</dcat:keyword>
