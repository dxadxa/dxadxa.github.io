    <dcterms:title xml:lang="fr">Taux de change effectif réel index (2010=100)</dcterms:title>
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/be94630e-1b54-46bf-b06b-a686793e4fc6"/>
