  <rdf:Description rdf:about="http://data.europa.eu/88u/document/da5175b7-fd8c-4ef1-bca9-666eb12bb81b">
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/ZIP"/>
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/documentation-type/DOCUMENTATION_RELATED"/>
