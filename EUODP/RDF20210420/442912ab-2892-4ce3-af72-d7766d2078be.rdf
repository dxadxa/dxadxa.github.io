    <dcterms:title xml:lang="en">Post-Friederike forest damage assessment in Germany, Hesse (2018-06-08)</dcterms:title>
    <dcterms:identifier>442912ab-2892-4ce3-af72-d7766d2078be</dcterms:identifier>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-06-08</dcterms:issued>
    <dcat:keyword xml:lang="en">Orthoimagery</dcat:keyword>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/d8c2bdbb-5e8f-485d-8cba-203cc276aee9"/>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/REGI"/>
