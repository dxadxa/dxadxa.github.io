    <dcterms:title xml:lang="de">Armutsgefährdungsquote vor Sozialleistungen nach Altersgruppe - EU-SILC Erhebung</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/c6b5d6d9-0da0-4ba4-9edf-cc06f91dcbeb"/>
