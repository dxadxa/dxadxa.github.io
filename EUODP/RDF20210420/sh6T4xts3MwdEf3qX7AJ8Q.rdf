    <dcterms:title xml:lang="fr">Personnes vivant dans un ménage à très faible intensité de travail par sexe</dcterms:title>
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/e6c2746c-adfa-4e33-a0dd-bd527344b699"/>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-10</dcterms:modified>
