  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/S1114_387">
    <dcterms:title xml:lang="en">Flash Eurobarometer 387: Les Français et l'Union européenne</dcterms:title>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-19</dcterms:issued>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/3a54def6-0588-418b-aa87-4bab861ef0bf"/>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/284eaa18-fb97-4150-8640-ec4230a6abd9"/>
