  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/LxpPlrYpewpJLfLxKqPtDA">
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/92224712-b526-457b-a54c-96734113f586"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/ba8f1dfe-f15d-42f5-b1da-a007c56b07d9"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/a7814e4a-7293-4d68-b374-d3b8bd3b4955"/>
