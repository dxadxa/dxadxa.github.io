  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/data-set-of-studies-on-mortality-sub-lethal-and-reproductive-effect-on-amphibians-and-reptiles">
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-07-19 14:48:24.801799</dcterms:modified>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/5877"/>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/9e600bcc-c739-47a6-b806-ccbed39ee257"/>
