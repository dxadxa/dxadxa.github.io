  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/a7d83242-81b3-4076-9ba9-61ec90ac8ec4">
    <dcat:accessURL rdf:resource="http://www.sesarju.eu/sites/default/files/solutions/3a_AOC_Data_for_TP_Concept.pdf?issuusl=ignore"/>
