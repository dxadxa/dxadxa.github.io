    <dcterms:title xml:lang="en">Process steam from natural gas 90%; heat plant; consumption mix, at power plant; MJ, 90 % efficiency (Location: GR)</dcterms:title>
    <dcatapop:isPartOfCatalog rdf:resource="http://data.europa.eu/88u/catalog/euodp"/>
