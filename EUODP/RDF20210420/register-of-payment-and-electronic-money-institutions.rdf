    <dcterms:title xml:lang="en">Register of payment and electronic money institutions</dcterms:title>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/EBA"/>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/1971"/>
