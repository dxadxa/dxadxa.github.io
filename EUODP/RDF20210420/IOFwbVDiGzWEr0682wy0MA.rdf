    <dcterms:title xml:lang="en">Share of landcover types affected by severe erosion (source: JRC)</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/fc11f9e2-4baf-45a0-855c-6ee8e9fe636b"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
