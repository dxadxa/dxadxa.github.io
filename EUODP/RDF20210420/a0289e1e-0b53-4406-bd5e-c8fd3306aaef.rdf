    <dcterms:title xml:lang="en">Floods in Bavaria, Germany (2016-06-02)</dcterms:title>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/0857445f-454a-4dca-bde3-c5b30f7d5ee0"/>
