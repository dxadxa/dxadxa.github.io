    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/4061c1e0-f3f5-4197-bb34-b116eadda25a"/>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/SOCI"/>
