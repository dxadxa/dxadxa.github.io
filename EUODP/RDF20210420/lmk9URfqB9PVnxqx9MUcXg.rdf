    <dcterms:title xml:lang="en">[DISCONTINUED] Expenditure on care for elderly</dcterms:title>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/e72e5e9e-1c54-49d4-825a-87753a704499"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/4f6f6eb0-624d-4802-8226-b390a774902d"/>
