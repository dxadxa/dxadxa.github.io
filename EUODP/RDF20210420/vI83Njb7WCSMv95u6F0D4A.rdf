    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/e6d327a2-7865-4e0b-9450-4f3ea59a8174"/>
    <dcterms:type rdf:resource="http://publications.europa.eu/resource/authority/dataset-type/STATISTICAL"/>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/e751b1ca-6b80-498f-9be0-ec36a808aed9"/>
