  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/5bcac7cf-bd9d-41d3-b08e-1cbbbdcecb64">
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-25</dcterms:modified>
