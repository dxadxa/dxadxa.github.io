    <dcterms:title xml:lang="en">Special Eurobarometer 118: Europeans and their views on child sex tourism</dcterms:title>
    <dcat:keyword xml:lang="en">sex</dcat:keyword>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/33cc8dac-e999-41dc-88dc-efc7dfa3fdf0"/>
