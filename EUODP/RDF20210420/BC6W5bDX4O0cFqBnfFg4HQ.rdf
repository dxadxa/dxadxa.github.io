    <dcterms:title xml:lang="en">Unemployment rate - 3 year average</dcterms:title>
    <dcterms:identifier>tipsun10</dcterms:identifier>
    <dcatapop:ckanName>BC6W5bDX4O0cFqBnfFg4HQ</dcatapop:ckanName>
