    <dcat:keyword xml:lang="en">exchange rate</dcat:keyword>
    <dcterms:title xml:lang="en">ECU/EUR exchange rates versus national currencies</dcterms:title>
    <dcterms:title xml:lang="de">ECU/EUR Wechselkurse gegen Landeswährungen</dcterms:title>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/feccac9e-367a-42a8-9fdd-c03c1c8079cf"/>
    <foaf:page rdf:resource="http://data.europa.eu/88u/document/3b8803fb-8d41-47de-bebe-1eeceb27dffc"/>
    <dcterms:description xml:lang="en">Exchange rates are the price or value of one country's currency in relation to another. Here the exchange rates are those for the euro published by the European Central Bank. Before 1999 the exchange rates are those of the ECU, as published by the European Commission.</dcterms:description>
