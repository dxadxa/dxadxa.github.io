  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/f80064ce-2687-41f9-b5e7-96db3256c503">
    <adms:status rdf:resource="http://publications.europa.eu/resource/authority/dataset-status/COMPLETED"/>
