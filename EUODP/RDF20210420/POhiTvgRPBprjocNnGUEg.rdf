    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/EDUC"/>
    <dcterms:publisher rdf:resource="http://publications.europa.eu/resource/authority/corporate-body/ESTAT"/>
    <dcterms:description xml:lang="en">Turnover is the total of all sales (excluding VAT) of goods and services carried out by the enterprises of a given sector during the reference period. The data is broken down by size classes of persons employed.</dcterms:description>
    <dcterms:title xml:lang="fr">Chiffre d'affaires de l'économie marchande non-financière par classe de taille d'entreprise</dcterms:title>
