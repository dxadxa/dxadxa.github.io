    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-18</dcterms:modified>
    <dcterms:title xml:lang="en">Turnover of the non-financial business economy by size class of employment</dcterms:title>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/cfb9f970-20b1-4c78-992c-a874e8c9b1d6"/>
