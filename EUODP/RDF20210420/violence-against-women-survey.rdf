  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/ef87b969-1090-4044-810d-851e52396b0b">
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-03-05 00:00:00</dcterms:issued>
