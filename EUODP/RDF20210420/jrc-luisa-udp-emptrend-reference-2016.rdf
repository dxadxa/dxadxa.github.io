    <dcterms:title xml:lang="en">UDP - Total employment by metro regions, 2000 - 2060 (JRC LUISA Reference Scenario 2016)</dcterms:title>
    <dcterms:spatial rdf:resource="http://publications.europa.eu/resource/authority/country/EST"/>
