    <dcterms:title xml:lang="en">Financial derivatives and employee stock options, by sectors - annual data, million units of national currency</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/738ec9bf-8c4f-44a4-ba44-9af642ba1afd"/>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/ECON"/>
