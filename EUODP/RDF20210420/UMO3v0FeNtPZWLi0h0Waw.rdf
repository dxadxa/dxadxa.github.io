    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/0b315f0e-097e-4a88-a57c-7340a4bf8a19"/>
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/228b5b3f-5a75-43b8-9f2e-5451142334a5"/>
    <dcterms:identifier>tesov016</dcterms:identifier>
    <dcterms:title xml:lang="fr">Ecart relatif médian pour le seuil de pauvreté - enquête EU-SILC</dcterms:title>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/c7717efc-d35f-40dc-b6b8-e9ba554a6953"/>
    <dcterms:subject rdf:resource="http://eurovoc.europa.eu/3659"/>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/EDUC"/>
    <dcterms:description xml:lang="en">The relative median at-risk-of-poverty gap is calculated as the difference between the median equivalised total net income of persons below the at-risk-of-poverty threshold and the at-risk-of-poverty threshold, expressed as a percentage of the at-risk-of-poverty threshold (cut-off point: 60% of median equivalised income). The EU aggregate is a population weighted average of individual national figures. In line with decisions of the European Council, the risk-of-poverty rate is measured relative to the situation in each country rather than applying a common threshold to all countries.</dcterms:description>
  </rdf:Description>
