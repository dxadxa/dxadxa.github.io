  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/xy6eyZjmxmULF20q9T9GkQ">
    <dcterms:title xml:lang="fr">Taux de chômage par sexe</dcterms:title>
    <dcterms:title xml:lang="de">Arbeitslosenquote nach Geschlecht</dcterms:title>
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/SOCI"/>
