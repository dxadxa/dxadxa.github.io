  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/YfHHApHuUMQuPfDWnmKSQ">
    <dcat:landingPage rdf:resource="http://data.europa.eu/88u/document/7ceda387-4ebf-4633-b849-114d2de32402"/>
