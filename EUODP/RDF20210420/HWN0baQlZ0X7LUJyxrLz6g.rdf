  <rdf:Description rdf:about="http://data.europa.eu/88u/dataset/HWN0baQlZ0X7LUJyxrLz6g">
    <dcterms:description xml:lang="en">This indicator is defined as the ratio between the volume of inland passenger transport measured in passenger-kilometres and GDP (chain-linked volumes, at 2010 exchange rates). It includes transport on national territory by passenger car, bus and coach, and train.</dcterms:description>
    <dcat:distribution rdf:resource="http://data.europa.eu/88u/distribution/d97da84e-2515-4cd4-9266-4f00ab77f2e3"/>
    <dcterms:title xml:lang="de">Umfang der Personenbeförderung im Verhältnis zum BIP</dcterms:title>
    <dcterms:identifier>ttr00001</dcterms:identifier>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-02-08</dcterms:modified>
    <dcterms:title xml:lang="en">Volume of passenger transport relative to GDP</dcterms:title>
    <dcat:contactPoint rdf:resource="http://www.w3.org/2006/vcard/ns#Kind/82556e8f-0800-4dc5-9e86-874ad1e54aeb"/>
    <dcterms:temporal rdf:resource="http://purl.org/dc/terms/PeriodOfTime/e9f1e1e8-3c87-4508-96b8-22d377d432a3"/>
