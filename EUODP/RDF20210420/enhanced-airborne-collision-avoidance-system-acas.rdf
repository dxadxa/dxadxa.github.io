    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/PDF"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/88u/distribution/423abd9e-3690-4f8e-a53b-c23cb70c498e">
    <rdf:type rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
    <dcterms:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/PDF"/>
    <dcterms:title xml:lang="en">5. SPR-TCAP</dcterms:title>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-01-19 10:49:25.805950</dcterms:issued>
    <dcterms:description xml:lang="en">The current report aims at presenting the safety and performance requirements for new altitude capture laws taking into account TCAS II thresholds to prevent the triggering of RAs in level-off encounters. These new altitude capture laws consist in reducing the own vertical speed automatically at the approach of the own selected flight level.</dcterms:description>
